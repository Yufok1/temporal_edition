{
  "name": "whale-communication-interface",
  "version": "1.0.0",
  "private": true,
  "description": "A system for whale-human communication and steward assessment",
  "main": "src/index.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:benchmarks": "jest --config=jest.integration.config.js src/__tests__/benchmarks/",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "start:auricle": "scripts/configure-system.sh start auricle-only",
    "start:whale": "scripts/configure-system.sh start whale-only",
    "start:crypto": "scripts/configure-system.sh start crypto-only",
    "start:dev": "scripts/configure-system.sh start development",
    "start:minimal": "scripts/configure-system.sh start minimal",
    "config:auricle": "scripts/configure-system.sh config auricle-only",
    "config:whale": "scripts/configure-system.sh config whale-only",
    "config:crypto": "scripts/configure-system.sh config crypto-only",
    "config:dev": "scripts/configure-system.sh config development",
    "config:minimal": "scripts/configure-system.sh config minimal",
    "system:status": "scripts/configure-system.sh status",
    "system:validate": "scripts/configure-system.sh validate"
  },
  "dependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.177.0",
    "dotenv": "^16.5.0",
    "libsodium-wrappers": "^0.7.15",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.10.3",
    "three": "^0.177.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bull": "^3.15.9",
    "@types/jest": "^27.5.2",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.0.3",
    "@types/sendgrid": "^2.0.31",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "bull": "^4.16.5",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^30.0.0",
    "redis": "^5.5.6",
    "ts-jest": "^29.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
